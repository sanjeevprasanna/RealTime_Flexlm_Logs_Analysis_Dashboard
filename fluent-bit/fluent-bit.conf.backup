[SERVICE]
    Flush        1
    Daemon       Off
    Log_Level    debug
    Parsers_File parsers.conf

[INPUT]
    Name         tail
    Path         /Users/sanjeevprasanna/projects/RealTimeLicenseMonitoringTool/logs/flexlm-all-scenarios.log
    Tag          flexlm.*
    Refresh_Interval 1
    Skip_Long_Lines  On
    DB                /Users/sanjeevprasanna/projects/RealTimeLicenseMonitoringTool/logs/flb_tails.db
    DB.Sync           Normal
    Read_from_Head   True

[FILTER]
    Name        parser
    Match       flexlm.*
    Key_Name    log
    Parser      flexlm_parser

[FILTER]
    Name        modify
    Match       flexlm.*
    Condition   Key_Value_Equals daemon synopsys
    Add         kafka_topic flexlm.logs.synopsys

[FILTER]
    Name        modify
    Match       flexlm.*
    Condition   Key_Value_Equals daemon cadence
    Add         kafka_topic flexlm.logs.cadence

[FILTER]
    Name        modify
    Match       flexlm.*
    Condition   Key_Value_Equals daemon altair
    Add         kafka_topic flexlm.logs.altair

[OUTPUT]
    Name        kafka
    Match       flexlm.*synopsys*
    Brokers     localhost:9092
    Topics      flexlm.logs.synopsys
    Format      json

[OUTPUT]
    Name        kafka
    Match       flexlm.*cadence*
    Brokers     localhost:9092
    Topics      flexlm.logs.cadence
    Format      json

[OUTPUT]
    Name        kafka
    Match       flexlm.*altair*
    Brokers     localhost:9092
    Topics      flexlm.logs.altair
    Format      json

